{
  "name": "passport-descope",
  "version": "1.0.0",
  "description": "Passport strategy for Descope authentication service",
  "typings": "dist/index.d.ts",
  "main": "dist/cjs/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.cjs.js",
    "import": "./dist/index.esm.js"
  },
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/descope/passport-descope.git"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c --bundleConfigAsCjs",
    "test": "jest",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier . --write --ignore-path .gitignore",
    "leaks": "bash gitleaks/gitleaks.sh"
  },
  "author": "Descope",
  "bugs": {
    "url": "git://github.com/descope/passport-descope/issues"
  },
  "license": "MIT",
  "keywords": [
    "descope",
    "authentication",
    "passport",
    "node"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.8",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/express": "5.0.4",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.10",
    "@types/passport": "1.0.17",
    "@types/node": "22.18.12",
    "@types/node-fetch": "2.6.13",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.4.0",
    "jest": "29.7.0",
    "prettier": "3.6.2",
    "rollup": "3.29.5",
    "rollup-plugin-delete": "2.2.0",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@descope/node-sdk": "1.6.10"
  },
  "peerDependencies": {
    "tslib": ">=1.14.1"
  }
}
